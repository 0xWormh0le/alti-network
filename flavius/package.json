  "scripts": {
    "start": "cross-env REACT_APP_STAGE=dev react-scripts start",
    "start-localdemo": "cross-env REACT_APP_STAGE=localdemo react-scripts start",
    "build-dev": "cross-env REACT_APP_STAGE=dev react-scripts build",
    "build-staging": "cross-env GENERATE_SOURCEMAP=false REACT_APP_STAGE=staging react-scripts build",
    "build": "cross-env GENERATE_SOURCEMAP=false REACT_APP_STAGE=production react-scripts build",
    "test": "cross-env TZ=America/Los_Angeles react-scripts test --env=jsdom-fourteen",
    "test-no-watch": "cross-env TZ=America/Los_Angeles CI=true react-scripts test --env=jsdom-fourteen --silent",
    "prettier": "pretty-quick --staged",
    "lint": "tsc && tslint 'src/**/*.{t,j}s?(x)'",
    "lint-fix": "tslint 'src/**/*.{t,j}s?(x)' --fix",
    "inspect-external-dependencies": "node ./.bin/assessBuild.js",
    "check-circular-dependencies": "node_modules/.bin/madge --circular --extensions ts src/",
    "generate-dependency-graph": "node_modules/.bin/madge --image assets/dependency-graph.svg --extensions ts src/",
    "coverage": "npm run test -- --coverage --watchAll=false && jest-coverage-badges && mv coverage/badge-statements.svg assets/coverage-badge.svg && git add .",
    "update-sentry": ". ./.bin/update-sentry.sh",
    "serve-e2e": "npm run serve &",
    "serve": "node_modules/.bin/serve -l 3000 build",
    "e2e-local": "node_modules/.bin/cypress open .",
    "e2e-travis": "node_modules/.bin/cypress run --record --key=$CYPRESS_RECORD_KEY",
    "e2e": "node_modules/.bin/cypress run",
    "gzip": "find build/ \\( -name '*.css' -o -name '*.html' -o -name '*.js' \\) -exec gzip --verbose --keep -9 {} \\;",
    "storybook": "start-storybook -p 6006 -c .storybook watch-css -s ./src/fonts,public,./src/icons,.storybook/addonAssets",
    "build-storybook": "build-storybook -s ./src/fonts,public,./src/icons,.storybook/addonAssets",
    "deploy-storybook": "storybook-to-aws-s3"
  },
  "devDependencies": {
    "@sentry/cli": "^1.63.2",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/node-logger": "^6.1.21",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.1.21",
    "@storybook/storybook-deployer": "^2.8.7",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.6",
    "@types/autosuggest-highlight": "^3.1.0",
    "@types/chartist": "^0.11.0",
    "@types/classnames": "^2.2.10",
    "@types/d3": "^5.16.3",
    "@types/d3-geo": "^2.0.0",
    "@types/file-saver": "^2.0.1",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^26.0.4",
    "@types/jest-when": "^2.7.1",
    "@types/lodash": "^4.14.157",
    "@types/md5": "^2.3.0",
    "@types/node": "^14.0.23",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/react-s-alert": "^1.3.2",
    "@types/react-select": "^3.0.14",
    "@types/segment-analytics": "^0.0.32",
    "@types/webpack": "^4.41.21",
    "@types/yup": "^0.29.3",
    "@types/zxcvbn": "^4.4.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-preset-react-app": "9.1.2",
    "browserslist": "^4.13.0",
    "chromedriver": "^89.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.11.0",
    "git-hooks": "^1.1.10",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-when": "^2.7.2",
    "madge": "^4.0.2",
    "msw": "^0.28.0",
    "msw-storybook-addon": "^1.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-scripts": "^4.0.3",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "serve": "^11.3.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "tsutils": "^3.21.0",
    "typescript": "^3.9.6",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      ".stories.tsx",
      ".mdx",
      ".storybook"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=10.16"
  },
  "msw": {
    "workerDirectory": ".storybook/addonAssets"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
