    "history": "^5.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "qrcode.react": "^1.0.0",
    "query-string": "^6.13.1",
    "react": "^17.0.2",
    "react-chartist": "^0.14.3",
    "react-content-loader": "^5.1.0",
    "react-dom": "^17.0.2",
    "react-flow-renderer": "^9.6.6",
    "react-google-login": "^5.1.21",
    "react-horizontal-scrolling-menu": "^0.7.7",
    "react-moment": "^0.9.7",
    "react-popper-tooltip": "^3.1.0",
    "react-router-dom": "5.2.0",
    "react-router-modal": "^2.0.0-rc3",
